ob1=OBI(cluster=1244)
ob1.reboot
ob1.start_diamond localhost
ob1.create_client
ob1.prepare
ob1.start_check
create table t1(a int primary key,b int);
insert into t1 values(-1,-1),(-2,-2),(0,0),(1,1);
select * from t1;
a	b
-2	-2
-1	-1
0	0
1	1
ob1.sh rm -rf `grep -i "dataid" -rl ~/.obdatasource/*`
update t1 set b=2 where a=1;
delete from t1 where a=0;
select * from t1;
a	b
-2	-2
-1	-1
1	2
drop table t1;
ob1.stop_check
ob1.stop_diamond
