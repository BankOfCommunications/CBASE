ob1=OBI(cluster=1211);
ob1.reboot;

ob1.mysql set global ob_read_consistency=3;
sleep 10;
ob1.mysql_p show variables like 'ob_read_consistency';

ob1.mysql_p select name,value from __all_sys_config_stat where name = 'read_master_master_ups_percent' or name = 'read_slave_master_ups_percent';

let $m_ups=deploy_get_value(ob1.get_master_ups);
let $s_ups=deploy_get_value(ob1.get_slave_ups);
ob1.$m_ups.status;
ob1.$s_ups.status;


ob1.create_client;
create table t1(pk int primary key, c1 int);

# worker
ob1.connect conn1;
set autocommit=0;
# make trouble
ob1.connect trouble;
set autocommit=0;
# checker
ob1.connect checker;
set autocommit=1;


# case 1): 3L,cmpd only,commit,outer 
connection trouble;
insert into t1 values(1,1);

connection conn1;
--error 5049
insert into t1 values(1,2) when row_count(insert into t1 values(2,2) when row_count(insert into t1 values(3,3))=1)=1;
commit;

connection trouble;
commit;

sleep 3;
connection checker;
--echo expect 1,1;
--repeat 20
select * from t1;
# clean
delete from t1 where pk=1;

ob1.$m_ups.status;
ob1.$s_ups.status;
ob1.$m_ups.exist;
ob1.$s_ups.exist;
ob1.$m_ups.nfind_log "checksum_error";
ob1.$s_ups.nfind_log "checksum_error";

# case 2): 3L,cmpd only,commit,inner 
connection trouble;
insert into t1 values(1,1);

connection conn1;
--error 5049
insert into t1 values(3,3) when row_count(insert into t1 values(2,2) when row_count(update t1 set c1=c1+1 where pk=1)=1)=1;
commit;

connection trouble;
commit;

sleep 3;
connection checker;
--echo expect 1,1;
--repeat 20
select * from t1;
# clean
delete from t1 where pk=1;

ob1.$m_ups.status;
ob1.$s_ups.status;
ob1.$m_ups.exist;
ob1.$s_ups.exist;
ob1.$m_ups.nfind_log "checksum_error";
ob1.$s_ups.nfind_log "checksum_error";

