ob1=OBI(cluster=1244);
ob2=OBI(cluster=1244);

ob2.rs0.set master=ob1.rs0;

ob1.reboot;
ob2.reboot;

sleep 60;
ob1.source include/merge.inc;
sleep 10;

ob1.prepare;
sleep 60;

ob1.start_check;

ob1.major_freeze;
ob1.check_cs_version 2;

ob1.stop_check;

ob1.force_set_slave_cluster ob2;
sleep 20;

ob1.start_check;
sleep 20;
ob1.stop_check;

ob1.force_switch_cluster ob2;
sleep 60;

ob1.start_check;
sleep 20;
ob1.stop_check;

create table t1(i1 int primary key, v1 varchar(20));

let $cnt=500;
while($cnt)
{
   eval insert into t1 values($cnt,'v$cnt');
   dec $cnt;	
}

let $cnt=500;
while($cnt)
{
   eval select * from t1  where i1=$cnt;
   dec $cnt;
}

let $cnt=500;
while($cnt)
{
   eval update t1 set v1='vv$cnt' where i1=$cnt;
   dec $cnt;
}

let $cnt=500;
while($cnt)
{
   eval select * from t1  where i1=$cnt;
   dec $cnt;
}

let $cnt=500;
while($cnt)
{
   eval delete from t1 where i1=$cnt;
   dec $cnt;
}

let $cnt=500;
while($cnt)
{
   eval select * from t1  where i1=$cnt;
   dec $cnt;
}

drop table t1;

