deploy ob1=OBI(cluster=1244, app_name=collect, schema_file=./collect.schema, local_sstable_dir=~/sstable);
deploy ob2=OBI(cluster=1244, app_name=collect, schema_file=./collect.schema, local_sstable_dir=~/sstable);


deploy ob2.rs0.set master=ob1.rs0;

deploy ob1.cs0.set sstable_dist={1,2};
deploy ob1.cs1.set sstable_dist={2,3};
deploy ob1.cs2.set sstable_dist={1,3};
deploy ob1.cs3.set sstable_dist=0;

deploy ob2.cs0.set sstable_dist={1,2};
deploy ob2.cs1.set sstable_dist={2,3};
deploy ob2.cs2.set sstable_dist={1,3};
deploy ob2.cs3.set sstable_dist=0;

deploy ob1.rs0.set extra_config=tablet_replicas_num=2,enable_balance=False;
deploy ob2.rs0.set extra_config=tablet_replicas_num=2,enable_balance=False;


deploy ob1.reboot;
deploy ob2.reboot;

sleep 60;

deploy ob1.source include/migrate.inc;

deploy ob1.prepare;
deploy ob1.start_check;
# wait for slave ob create tablet
sleep 60;

deploy ob1.check_cs_sstable 404;
deploy ob2.check_cs_sstable 404;

alter system set tablet_replicas_num=3 server_type=rootserver;
deploy ob1.cs3.check_log_until_success "begin migrate data";
deploy ob1.cs3.restart;
sleep 120;
deploy ob1.check_cs_sstable 606;
deploy ob2.check_cs_sstable 606;

deploy ob1.stop_check;
