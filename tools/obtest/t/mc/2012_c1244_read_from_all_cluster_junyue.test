deploy ob1=OBI(cluster=1244);
deploy ob2=OBI(cluster=1244);

deploy ob2.rs0.set master=ob1.rs0;

deploy ob1.reboot;
deploy ob2.reboot;

sleep 60;
deploy ob1.mysql set global ob_read_consistency=3;
let $c=deploy_get_value(ob1.mysql_p select cluster_id,cluster_role,cluster_flow_percent from __all_cluster);
--echo $c
deploy ob1.mysql update __all_cluster set cluster_flow_percent=50 where cluster_id=1;
deploy ob1.mysql update __all_cluster set cluster_flow_percent=50 where cluster_id=2;
sleep 10;

deploy ob1.create_client;

#--disable_query_log

let $cnt=100;
while($cnt)
{
    eval create table t$cnt(c1 int primary key, c2 int);
    dec $cnt;
}

sleep 60;
let $cnt=1000;
while($cnt > 500)
{
  eval replace into t1 values($cnt,$cnt);
  dec $cnt;
}
while($cnt)
{
  eval insert into t1 values($cnt,$cnt);
  dec $cnt;
}

let $cnt=1000;
while($cnt > 500)
{
  eval update t1 set c2=1 where c1=$cnt;
  dec $cnt;
}
while($cnt)
{
  eval delete from t1 where c1=$cnt;
  dec $cnt;
}


# wait for slave ob create tablet
#sleep 60;
let $cnt=1000;
while($cnt)
{
  --disable_result_log
  eval select * from t1 where c1=$cnt;
  dec $cnt;
}

let $cnt=100;
while($cnt)
{
    eval drop table t$cnt;
    dec $cnt;
}

