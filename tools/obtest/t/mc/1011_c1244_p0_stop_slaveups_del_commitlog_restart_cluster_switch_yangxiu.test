deploy ob1=OBI(cluster=1244);
deploy ob2=OBI(cluster=1244);
deploy ob2.rs0.set master=ob1.rs0;

deploy ob1.reboot;
deploy ob2.reboot;
sleep 60;

deploy ob1.prepare s;

# new data
deploy ob1.create_client;
create table t1(c1 int primary key,c2 int);

# wait slave cluster cs create tablet
sleep 60;
insert into t1 values(1,1),(2,2),(3,3),(4,4);

# scan ups
select * from t1;

# master_master_ups stop
let $master_ups=deploy_get_value(ob2.get_master_ups);
deploy ob2.$master_ups.force_stop;

deploy ob2.$master_ups.clean_commit_log;
sleep 20; 

#sql
insert into t1 values(5,1),(6,2),(7,3),(8,4);
select * from t1;
create table t2(a int primary key,b varchar(10));
sleep 60;
insert into t2 values(1,'test1'),(2,'test2');
select * from t2;

#restart master_master_ups
deploy ob2.$master_ups.start;
sleep 10;

#cluster switch
deploy ob1.switch_cluster ob2;

#sql
update t1 set c2=10 where c1=8;
select * from t1;
select * from t2;

