case 1: commit
show variables like 'autocommit';
variable_name	value
autocommit	1
drop table if exists t1;
create table t1 (c1 int primary key, c2 int);
set autocommit=0;
insert into t1 values (1, 2);
select * from t1 where c1 = 1 for update;
c1	c2
1	2
commit;
set autocommit=1;
select * from t1;
c1	c2
1	2
