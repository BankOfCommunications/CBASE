--disable_abort_on_error
--disable_warnings
drop table if exists t1;
--enable_warnings

--let $value=1
--let $kb=64
--let $
while($kb)
{
    --let $value=$value$byte
    dec $kb;
}


# insert one each time
create table t1(pk int primary key, f1 varchar(65535));

--let $row=50

while($row)
{
   eval insert into t1 values($row, $value);
   dec $row;
}

select * from t1;
drop table t1;

# insert all
create table t1(pk int primary key, f1 varchar(65535));

--let $insert_sql=insert into t1 values(-1,'1')
--let $row=50

while($row)
{
   --let $insert_sql=$insert_sql,($row, $value);
   dec $row;
}
eval $insert_sql;

select * from t1;

drop table t1;
