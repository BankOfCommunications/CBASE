INSERT INTO t2 VALUES (2, 2,'abcde');
SELECT sb1,sb2,sb3 FROM (SELECT c1 AS sb1, c2 AS sb2, c3*2 AS sb3
FROM t1) AS sb WHERE sb1 > 1;
SELECT AVG(sum_column1) FROM (SELECT SUM(c1) AS sum_column1 FROM t1
 GROUP BY c1) AS t1;
