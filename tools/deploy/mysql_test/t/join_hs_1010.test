select /*+ join(bloomfilter_join) */ * from t1 left join t2 on t1.id=t2.id and t1.c1=t2.id and t1.c2=t2.id and t1.c3>t2.c4;
select /*+ join(bloomfilter_join) */ * from t1 right join t2 on t1.id=t2.id and t1.c1=t2.id and t1.c2=t2.id and t1.c3>t2.c4;
select /*+ join(bloomfilter_join) */ * from t1 inner join t2 on t1.id=t2.id and t1.c1=t2.id and t1.c2=t2.id and t1.c3>t2.c4;
select /*+ join(bloomfilter_join) */ * from t1 full join t2 on t1.id=t2.id and t1.c1=t2.id and t1.c2=t2.id and t1.c3>t2.c4;

select /*+ join(merge_join) */ * from t1 left join t2 on t1.id=t2.id and t1.c1=t2.id and t1.c2=t2.id and t1.c3>t2.c4;
select /*+ join(merge_join) */ * from t1 right join t2 on t1.id=t2.id and t1.c1=t2.id and t1.c2=t2.id and t1.c3>t2.c4;
select /*+ join(merge_join) */ * from t1 inner join t2 on t1.id=t2.id and t1.c1=t2.id and t1.c2=t2.id and t1.c3>t2.c4;
select /*+ join(merge_join) */ * from t1 full join t2 on t1.id=t2.id and t1.c1=t2.id and t1.c2=t2.id and t1.c3>t2.c4;

drop table t1,t2,t3,t4;
