--source $LOCAL_DIR/mysql_test/import-export/include.inc 

--let $TABLE=ie_decimal
--let $DB=test
--let $TEST_NAME=$TABLE
--let $IM_FILE=$IM_DEL/$TEST_NAME.del
--let $EX_FILE=$EX_DEL/$TEST_NAME.del

eval using database $DB;
# --disable_warnings
eval drop table if exists $TABLE;
# --enable_warnings

eval create table $TABLE
(

it int,
vc1 varchar(256),

dc1 decimal(30,0),
dc2 decimal(30,5),
vc2 varchar(256),
primary key(it)

);

exec $IM2_EXE
-t $TABLE 
-h $RS0_IP 
-p $RS0_PORT 
       -l $IM2_LOG/$TABLE.log 
--badfile $IM2_BAD/$TABLE.bad 
--dbname $DB 
-f $IM_FILE
&> /dev/null;

eval select * from $TABLE;

exec $EX_EXE --masterpercent 100 
-t $DB.$TABLE 
-h $RS0_IP 
-p $RS0_PORT 
       -l $EX_LOG/$TABLE.log 
--badfile $EX_BAD/$TABLE.bad 
-f $EX_FILE
&> /dev/null;

exec cat $EX_FILE;
