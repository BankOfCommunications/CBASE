##
#
create table t1 (col1 double primary key, col2 double);
insert into t1(col1) values (-5.00000000001);
insert into t1(col1) values (-5.00000000002);
select col1,sum(col1),max(col1),min(col1) from t1 group by col1;
#delete from t1;
insert into t1(col1) values (5.00000000003);
insert into t1(col1) values (5.00000000004);
select col1,sum(col1),max(col1),min(col1) from t1 group by col1;
DROP TABLE t1;
